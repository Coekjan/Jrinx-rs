{
    "llvm-target": "riscv32",
    "data-layout": "e-m:e-p:32:32-i64:64-n32-S128",
    "target-endian": "little",
    "target-pointer-width": "32",
    "target-c-int-width": "32",
    "code-model": "medium",
    "os": "none",
    "arch": "riscv32",
    "cpu": "generic-rv32",
    "features": "+m,+a,+c",
    "max-atomic-width": "32",
    "linker": "rust-lld",
    "linker-flavor": "ld.lld",
    "pre-link-args": {
        "ld.lld": [
            "-Tkern/tgt/riscv32.ld"
        ]
    },
    "executables": true,
    "panic-strategy": "abort",
    "relocation-model": "static"
}
